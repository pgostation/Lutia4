#   File:       lutia4.make#   Target:     lutia4#   Created:    Sunday, January 18, 2004 12:59:22 AMMAKEFILE        = lutia4.make쩗ondoBuild    = {MAKEFILE}  # Make blank to avoid rebuilds when makefile is modifiedObjDir          = :MPW_Object:Includes        = Sym-PPC         = -sym offPPCCOptions     = {Includes} {Sym-PPC} -d TARGET_API_MAC_CARBON=1 -j0### Source Files ###SrcFiles        =  				  draw.c 				  game.c 				  key.c 				  macWrap.c 				  main.c 				  util.c 				  sprite.c### Object Files ###ObjFiles-PPC    =  				  "{ObjDir}draw.c.x" 				  "{ObjDir}game.c.x" 				  "{ObjDir}key.c.x" 				  "{ObjDir}macWrap.c.x" 				  "{ObjDir}main.c.x" 				  "{ObjDir}util.c.x" 				  "{ObjDir}sprite.c.x"### Libraries ###LibFiles-PPC    =  				  "{PPCLibraries}CarbonStdCLib.o" 				  "{SharedLibraries}CarbonLib" 				  "{SharedLibraries}StdCLib" 				  "{PPCLibraries}StdCRuntime.o" 				  "{PPCLibraries}PPCCRuntime.o" 				  "{PPCLibraries}PPCToolLibs.o"### Default Rules ###.c.x    .c  {쩗ondoBuild}	{PPCC} {depDir}{default}.c -o {targDir}{default}.c.x {PPCCOptions}### Build Rules ###lutia4  컴  {ObjFiles-PPC} {LibFiles-PPC} {쩗ondoBuild}	PPCLink 		-o {Targ} 		{ObjFiles-PPC} 		{LibFiles-PPC} 		{Sym-PPC} 		-mf -d 		-m __appstart 		-t 'APPL' 		-c 'OCza'lutia4  컴  mpw.rsrc {쩗ondoBuild}	Echo "Include "mpw.rsrc";" | Rez -o {Targ} -appendlutia4  컴  {쩗ondoBuild}	Echo "Data 'carb' (0) {$"00000000"};" | Rez -o {Targ} -appendlutia4  컴  {쩗ondoBuild}	Echo 		"#include "Processes.r"턭 		Resource 'SIZE' (-1) { 		reserved, 		acceptSuspendResumeEvents, 		reserved, 		canBackground, 		doesActivateOnFGSwitch, 		backgroundAndForeground, 		dontGetFrontClicks, 		ignoreAppDiedEvents, 		is32BitCompatible, 		notHighLevelEventAware, 		onlyLocalHLEvents, 		notStationeryAware, 		dontUseTextEditServices, 		notDisplayManagerAware, 		reserved, 		reserved, 		8000 * 1024, 		4000 * 1024 };" | Rez -o {Targ} -append### Required Dependencies ###"{ObjDir}draw.c.x"    draw.c"{ObjDir}game.c.x"    game.c"{ObjDir}key.c.x"    key.c"{ObjDir}macWrap.c.x"    macWrap.c"{ObjDir}main.c.x"    main.c"{ObjDir}util.c.x"    util.c"{ObjDir}sprite.c.x"    sprite.c### Optional Dependencies ###### Build this target to generate "include file" dependencies. ###Dependencies    $OutOfDate	MakeDepend 		-append {MAKEFILE} 		-ignore "{CIncludes}" 		-objdir "{ObjDir}" 		-objext .x 		{Includes} 		{SrcFiles}